<h1>{{title}}</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nome</th>
            <th scope="col">Matricula</th>
            <th scope="col">Data de Nascimento</th>
            <th scope="col">Email</th>
            <th scope="col"></th>
            
        </tr>
    </thead>
    <tbody>
        {{#each alunos.content}}
        <tr alunos.content>
            <th><a class="btn btn-success" href="/alunos/{{matricula}}">{{nome}}</a></th>
            <th>{{matricula}}</th>
            <th>{{data_nascimento}}</th>
            <th>{{email}}</th>
            <td><a class="btn btn-info" href="/alunos/edit/{{matricula}}">editar</a></td>
        </tr>
        {{/each}}
    </tbody>
</table> 

<a class="btn btn-info" href="/alunos/new">Adcionar Novo Aluno</a>